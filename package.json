    "fmt": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@iconify/svelte": "^4.0.2",
    "@playwright/test": "^1.48.2",
    "@poppanator/sveltekit-svg": "^5.0.0",
