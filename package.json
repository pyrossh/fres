    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@sentry/sveltekit": "^7.102.0",
    "@unocss/reset": "^0.58.5",
    "remark-github": "^12.0.0",
